#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/tadaspetraitis/Desktop/test/node_modules/.pnpm/@hungrimind+hungrimind-cli@0.0.3/node_modules/@hungrimind/hungrimind-cli/bin/node_modules:/Users/tadaspetraitis/Desktop/test/node_modules/.pnpm/@hungrimind+hungrimind-cli@0.0.3/node_modules/@hungrimind/hungrimind-cli/node_modules:/Users/tadaspetraitis/Desktop/test/node_modules/.pnpm/@hungrimind+hungrimind-cli@0.0.3/node_modules/@hungrimind/node_modules:/Users/tadaspetraitis/Desktop/test/node_modules/.pnpm/@hungrimind+hungrimind-cli@0.0.3/node_modules:/Users/tadaspetraitis/Desktop/test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/tadaspetraitis/Desktop/test/node_modules/.pnpm/@hungrimind+hungrimind-cli@0.0.3/node_modules/@hungrimind/hungrimind-cli/bin/node_modules:/Users/tadaspetraitis/Desktop/test/node_modules/.pnpm/@hungrimind+hungrimind-cli@0.0.3/node_modules/@hungrimind/hungrimind-cli/node_modules:/Users/tadaspetraitis/Desktop/test/node_modules/.pnpm/@hungrimind+hungrimind-cli@0.0.3/node_modules/@hungrimind/node_modules:/Users/tadaspetraitis/Desktop/test/node_modules/.pnpm/@hungrimind+hungrimind-cli@0.0.3/node_modules:/Users/tadaspetraitis/Desktop/test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/run.js" "$@"
else
  exec node  "$basedir/../../bin/run.js" "$@"
fi
